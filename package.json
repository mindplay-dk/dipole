{
  "name": "dipole",
  "version": "2.0.0",
  "description": "Dipole - tiny reactive state management that just works",
  "main": "dist/dipole.js",
  "module": "dist/dipole.mjs",
  "unpkg": "dist/dipole.umd.js",
  "types": "./src/dipole.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/dipole.js",
      "umd": "./dist/dipole.umd.js",
      "require": "./dist/dipole.js",
      "import": "./dist/dipole.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zheksoon/dipole.git"
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "jest": "^26.6.1",
    "microbundle": "^0.12.4"
  },
  "scripts": {
    "test": "jest",
    "build": "npx microbundle build src/dipole.js"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "observable",
    "observables",
    "computed",
    "reactive",
    "state management",
    "functional reactive programming",
    "frp",
    "tfrp",
    "mobx"
  ],
  "minify": {
    "mangle": {
      "reserved": [
        "HashSet",
        "Observable",
        "Computed",
        "Reaction"
      ]
    }
  },
  "author": "Eugene Daragan",
  "license": "MIT"
}
